{
  "name": "movies_frontend",
  "private": true,
  "version": "1.0.25",
  "type": "module",
  "scripts": {
    "commit": "cz",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix && stylelint --fix '**/*.(css|scss|tsx)'",
    "format": "prettier --write .",
    "test": "NODE_NO_WARNINGS=1 jest --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "test:badges": "yarn test:coverage && coverage-badges --save=./badges"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.7.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/react-dom": "^19.1.2",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "formik": "^2.4.6",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@adso-ts/badges": "^1.1.4",
    "@eslint/js": "^9.25.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fixed-jsdom": "^0.0.9",
    "license-generator": "^0.0.13",
    "lint-staged": "^15.5.1",
    "msw": "^2.7.5",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.5.3",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "ts-jest": "^29.3.2",
    "ts-jest-mock-import-meta": "^1.3.0",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{jsx,tsx,css,scss}": [
      "stylelint --fix"
    ],
    "*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
