{
  "name": "movies_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "commit": "cz",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix && stylelint --fix '**/*.(css|scss|tsx)'",
    "format": "prettier --write .",
    "test": "NODE_NO_WARNINGS=1 jest --passWithNoTests",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage && coverage-badges --save=./badges"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "@types/react-dom": "^18.3.1",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "formik": "^2.4.6",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@adso-ts/badges": "^1.1.4",
    "@eslint/js": "^9.9.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-fixed-jsdom": "^0.0.2",
    "license-generator": "^0.0.13",
    "lint-staged": "^15.2.10",
    "msw": "^2.5.2",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "ts-jest": "^29.2.5",
    "ts-jest-mock-import-meta": "^1.2.0",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.11.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{jsx,tsx,css,scss}": [
      "stylelint --fix"
    ],
    "*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
